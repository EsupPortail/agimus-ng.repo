objectclass ( 1.3.6.1.4.1.7135.1.2.2.6 NAME 'supannGroupe' SUP top AUXILIARY
        DESC 'attributs specifiques des groupes'
        MAY ( supannGroupeDateFin  $ supannGroupeAdminDN $ supannGroupeLecteurDN ) )

